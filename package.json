{
  "name": "honeylight-editor",
  "version": "0.0.1",
  "description": "Honeylight pattern editor",
  "private": true,
  "scripts": {
    "clean": "rimraf out && mkdirp out",
    "lint": "eslint src --fix",
    "flow": "flow 2>&1",
    "stylelint": "stylelint \"src/scss/**/*.scss src/fonts/*.scss\" --syntax scss --fix",
    "build:dev": "webpack --color --progress --mode development",
    "build": "webpack --color --progress --mode production",
    "build:ci": "webpack --color --mode production",
    "devserver": "webpack serve --mode development",
    "validate": "run-p --aggregate-output -c clean lint flow stylelint",
    "dev": "run-s validate build:dev",
    "debug": "run-s validate devserver",
    "update-flow-types": "flow-typed update"
  },
  "engines": {
    "node": ">=14"
  },
  "author": "Shame Boi <shameboi69@gmail.com>",
  "license": "AGPL-3.0-or-later",
